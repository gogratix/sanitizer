{
    "name": "5150studios/sanitizer",
    "description": "Data sanitizer for form requests supporting Laravel 8.",
    "keywords": ["laravel", "laravel 8", "sanitation", "input sanitation", "input sanitizer", "input", "transform input", "input filter"],
    "license": "MIT",
    "authors": [
        {
            "name": "William Wallace San Paulo",
            "email": "info@waavi.com"
        },
        {
            "name": "Mick Davies",
            "email": "mick@5150studios.com.au"
        }
    ],
    "require": {
        "illuminate/support": "~7.0|^8.0",
        "illuminate/validation": "~7.0|^8.0",
        "nesbot/carbon": "~2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "5150studios\\Sanitizer\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": ["tests"]
    },
    "extra": {
        "laravel": {
            "providers": [
                "5150studios\\Sanitizer\\Laravel\\SanitizerServiceProvider"
            ],
            "aliases": {
                "Sanitizer": "5150studios\\Sanitizer\\Laravel\\Facade"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
